@model QualifyMe.ViewModels.JobView

<div class="col-sm-12">
    
        
            <div class="block">

                <img src="~/images/avatar.png" alt="" height="50px" width="50px">
                <div>
                    <p>@Model.Company.CompanyName</p>

                    <a href="/jobs/view/@Model.CompanyID">@Model.JobTitle</a>
                </div>
                <p>@Model.Department.DepartmentName</p>
                <a href="/company/applicants/list">No. Of Applicants : @Model.ApplicantsCount</a>
            </div>
        </div>
   
   






