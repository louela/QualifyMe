
@model List<QualifyMe.ViewModels.CompanyView>

@{
    ViewBag.Title = "Companies";
}





<div id="layoutSidenav_content">
    <main>

        <div class="col-sm-12">
            <h4>List of Companies</h4>

            <a class="nav-link" href="/admin/companies/addcompany">Add Company</a>

            <table class="table table-striped table-hover">
                <thead>
                <tr>

                    <th>Company</th>
                    <th>Email</th>
                </tr>
                </thead>
                @foreach (var item in Model)
                {
                    <tbody>
                    <tr>

                        <td>@item.CompanyName</td>
                        <td>@item.Email</td>
                    </tr>
                    </tbody>
                }
            </table>
        </div>

    </main>

</div>

