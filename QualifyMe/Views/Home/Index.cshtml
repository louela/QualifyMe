@model List<QualifyMe.ViewModels.JobView>

@{ 
    ViewBag.Title = "Home";
}
<div class="row">
    <div class="col-sm-12">
        <div class="jumbotron">
            <h1>Work while Studying, QualifyME!</h1>
            <h3>Get your career on path with QualifyME!.</h3>
            <p class="lead">Get career opportunities on QualifyMe.</p>
        </div>
    </div>
</div>

<div class="row">
    <div class="col-sm-12">
        <table class="table">
            @foreach(var item in Model)
            {
                Html.RenderPartial("Job", item);
            }
        </table>

    </div>
</div>

<script>
    $("a[href='/home/index']").attr("id", "active");
</script>